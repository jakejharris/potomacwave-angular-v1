{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist/potomacwave-angular-v1/browser" }
      }
    ],
    "routes": [
      { "src": "/assets/(.*)", "dest": "/assets/$1" },
      { "src": "/(.*).(js|css|ico|json)", "dest": "/$1.$2" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }